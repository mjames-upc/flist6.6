<%= simple_form_for(@franken_beer) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :beername %>
    <%= f.input :active %>
    <%= f.input :rbid %>
    <%= f.association :franken_brewery %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
