<%- model_class = FrankenBrewery -%>
<div class="page-header">
  <h1><%=t '.title', :default => FrankenBrewery %></h1>
</div>

<div class="fieldset">
  <dl>
      <dt class="label label-default"><%= model_class.human_attribute_name(:brewer_name) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.brewer_name %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:address) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.address %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:city) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.city %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:district) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.district %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:rbid) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.rbid %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:active) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.active %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:founded) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.founded %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:brewer_type) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.brewer_type %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:score) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.score %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:lat) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.lat %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:lon) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.lon %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:web) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.web %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:hours) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.hours %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:description) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.description %></pre></dd>
      <dt class="label label-default"><%= model_class.human_attribute_name(:phone) %></dt>
      <dd><pre class="prettyprint"><%= @franken_brewery.phone %></pre></dd>
  </dl>
</div>

<div class="form-group">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              franken_breweries_path, :class => 'btn btn-default'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_franken_brewery_path(@franken_brewery), :class => 'btn btn-default' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              franken_brewery_path(@franken_brewery),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>

